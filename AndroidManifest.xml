<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.fernandez.clotheselection"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat" >
        <activity
            android:name="org.fernandez.clotheselection.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.fernandez.clotheselection.TopBottomActivity"
            android:label="@string/title_activity_top_bottom" 
            >
        </activity>
        <activity
            android:name="org.fernandez.clotheselection.TopActivity"
            android:label="@string/title_activity_top" >
        </activity>
        <activity
            android:name="org.fernandez.clotheselection.BottomActivity"
            android:label="@string/title_activity_bottom" >
        </activity>
    </application>
</manifest>
